{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        // eslint rules
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": "off",
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "no-param-reassign": "off",
        "no-use-before-define": "off",
        "no-shadow": "off"
    }
}
